<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="540" d:DesignHeight="460"
			 Width="540" Height="460"
             x:Class="mrKrrabs.Views.ResultView">
	<DockPanel Margin="30" Background="PaleGoldenrod">
		<StackPanel Background="Azure" DockPanel.Dock="Top">
			<TextBlock FontSize="20" FontWeight="Medium">Hasil Pencarian</TextBlock>
			<TextBlock FontSize="20" FontWeight="Medium">Algoritma</TextBlock>
			<TextBlock>TSP ya</TextBlock>
			<TextBlock>Banyak langkah 6</TextBlock>
			<TextBlock>Nodes 6</TextBlock>
			<TextBlock>Route R - D - D - R - R - U</TextBlock>
			<TextBlock>Waktu eksekusi 800ms</TextBlock>
		</StackPanel>
		<Button DockPanel.Dock="Bottom" HorizontalAlignment="Center" VerticalAlignment="Bottom" Command="{Binding $parent[Window].DataContext.BeginForm}">
			Kembali
		</Button>
	</DockPanel>
</UserControl>
